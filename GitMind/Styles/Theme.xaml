													</VisualStateGroup>
												</VisualStateManager.VisualStateGroups>
												<Grid HorizontalAlignment="Stretch" Margin="0,0,0,0" VerticalAlignment="Stretch" Width="Auto" Height="Auto" x:Name="NormalState">
